{
  "name": "@littlepony/redux-ws-jsonrpc",
  "version": "0.0.5",
  "description": "Redux with JSON-RPC over websocket middleware",
  "main": "dist",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --coverage",
    "build": "npm run clean && npm run build:types && webpack",
    "build:types": "tsc --emitDeclarationOnly --outdir dist",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LittlePony/redux-ws-jsonrpc.git"
  },
  "keywords": [
    "redux",
    "websocket",
    "jsonrpc"
  ],
  "author": "LittlePony",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LittlePony/redux-ws-jsonrpc/issues"
  },
  "homepage": "https://github.com/LittlePony/redux-ws-jsonrpc#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "7.10.1",
    "@types/jest": "^26.0.0",
    "@types/ws": "^7.2.5",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "babel-loader": "8.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-promise": "4.2.1",
    "flux-standard-action": "^2.1.1",
    "jest": "^26.0.1",
    "jest-websocket-mock": "^2.0.2",
    "mock-socket": "^9.0.3",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "redux": "~4"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  }
}
